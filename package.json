{
	"name": "t-nextjs",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "biome lint .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"test": "vitest run",
		"coverage": "vitest run --coverage",
		"bench": "vitest bench",
		"prepare": "husky",
		"init": "npm install pnpm -g && pnpm install && pnpm test && pnpm dev",
		"git-prepare": "pnpm format:fix && pnpm lint && pnpm test && pnpm coverage && pnpm build"
	},
	"dependencies": {
		"next": "14.2.4",
		"react": "^18",
		"react-dom": "^18"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.1",
		"@testing-library/jest-dom": "^6.4.6",
		"@testing-library/react": "^16.0.0",
		"@testing-library/react-hooks": "^8.0.1",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@vitejs/plugin-react": "^4.3.1",
		"@vitest/coverage-v8": "^1.6.0",
		"happy-dom": "^14.12.0",
		"husky": "^9.0.11",
		"jsdom": "^24.1.0",
		"postcss": "^8",
		"tailwind-merge": "^2.3.0",
		"tailwindcss": "^3.4.1",
		"typescript": "^5",
		"vitest": "^1.6.0"
	}
}
